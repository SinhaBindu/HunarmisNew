@using System.Data
@using System.Text.RegularExpressions;
@using Hunarmis.Manager
@{
    DataTable dt = new DataTable();
    dt = Model;
}
<style>

</style>
    <div class="table-responsive">
        <table class="table table-striped nowrap table-bordered dataTable exampleDatatable no-footer" id="tbl">
            <thead class="text-center">
                <tr>
                    <th class="text-center tblth">Srno.</th>
                    <th class="text-center tblth">Date</th>
                    <th class="text-center tblth">File Name</th>

                    <th class="text-center tblth">Total Data</th>
                    <th class="text-center tblth">Imported Data</th>
                    <th class="text-center tblth">File DownLoad</th>
                </tr>
            </thead>
            <tbody>
                @{
                    if (dt.Rows.Count > 0)
                    {
                        int srno = 1;
                        foreach (DataRow dr in dt.Rows)
                        {
                            <tr>
                                <td class="text-center">@(srno++)</td>
                                <td class="text-center bg-success text-white fw-bold"><strong>@dr["UploadDate"]</strong></td>
                                <td class="text-left"> @dr["FileName"] </td>
                                <td class="text-center"> @dr["TotalData"] </td>
                                <td class="text-center"> @dr["NoofInserted"] </td>
                                <td class="text-center  bg-success text-white fw-bold">
                                    <a href="@(Hunarmis.Manager.CommonModel.GetBaseUrl()+dr["FilePathDownload"])"><strong><i class="fa fa-download" style=" margin-left: 55% !important;"></i></strong></a>
                                </td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="4" class="text-center"><strong class="text-danger">Record Not Found !!</strong></td>
                        </tr>
                    }
                }
            </tbody>

        </table>
    </div>

