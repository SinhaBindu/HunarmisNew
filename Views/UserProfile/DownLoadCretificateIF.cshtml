@using Hunarmis.Models
@using Hunarmis.Manager
@{
    string url = string.Empty;
    Courses_Master clist = new Courses_Master();
    if (Model != null)
    {
        clist = Model;
    }
}
<style>
    #end{
        display:none !important;
    }
</style>

<script>
    
    //window.oncontextmenu = function () {
    //    return false;
    //}
    //window.frames["iframe1"].contentDocument.oncontextmenu = function () {
    //    return false;
    //};
    document.addEventListener("contextmenu", function (e) {
        e.preventDefault();
    });
    document.addEventListener("keydown", function (e) {
        if (e.ctrlKey || e.metaKey) {
            e.preventDefault();
        }
    });
    document.getElementById("iframe1").addEventListener("keydown", function (e) {
        if (e.ctrlKey || e.metaKey) {
            e.preventDefault();
        }
    });
</script>
<div id="wrapper">
    @if (clist != null)
    {
        <iframe id="iframe1" src="@(CommonModel.GetBaseUrl()+clist.FilePath)#toolbar=0&#print=0"  name="iframe1" scrolling="yes" height="600" width="100%" marginwidth="0" marginheight="0"
                onload="disableContextMenu();" title=""></iframe>
        @*<embed src="@(CommonModel.GetBaseUrl()+clist.FilePath)#view=FitH" style="width:100%; height:700px;">*@
    }
</div>
